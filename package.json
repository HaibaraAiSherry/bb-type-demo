{
  "name": "bb-typewrite",
  "description": "BB打字练习平台",
  "private": true,
  "packageManager": ">=pnpm@7.12.0",
  "engines": {
    "node": "=17.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "type-check": "tsx ./scripts/type-check.ts",
    "web:dev": "cd packages/web && pnpm dev",
    "web:build": "cd packages/web && pnpm build",
    "desktop:dev": "cd packages/desktop && pnpm dev",
    "lint": "npm run type-check && lint-staged",
    "lint-fix": "eslint ./ --fix",
    "clean": "rimraf **/node_modules",
    "api-schema:update": "tsx ./scripts/api-schema-update.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@commitlint/types": "^17.0.0",
    "@mimi-utils/types": "^0.0.3",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "^14.3.0",
    "@type-challenges/utils": "^0.1.1",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-react": "^1.3.2",
    "cypress": "^10.4.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "execa": "6.1.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "openapi-typescript": "^5.4.1",
    "picocolors": "^1.0.0",
    "prettier": "3.0.0-alpha.1",
    "react": "^18.2.0",
    "react-dev-inspector": "^1.8.1",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-toolbelt": "^9.6.0",
    "tsx": "^3.8.0",
    "type-fest": "^2.16.0",
    "typescript": "4.9.0-dev.20221007",
    "vite": "^3.1.7",
    "vite-plugin-svgr": "^2.2.0",
    "vitest": "^0.24.0"
  },
  "dependencies": {
    "mitt": "^3.0.0",
    "super-hotkey": "file:../super-hotkey"
  },
  "resolutions": {},
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint . --ext ts --ext tsx"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
